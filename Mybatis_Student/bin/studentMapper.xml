<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 학생정보 처리 SQL -->
<mapper namespace="student.dao.StudentMapper">

<!-- 학생 정보 저장 -->
<insert id="insert" parameterType="Student">
	insert into
		student(
			id
			,name
			,kor
			,eng
			,mat
		)values(
			#{id}
			,#{name}
			,#{kor}
			,#{eng}
			,#{mat}
		)
</insert>

<!-- 전체 학생 목록 읽기 -->


<!-- 학번으로 학생정보 삭제 -->


<!-- 학생 정보 수정 -->


<!-- 이름으로 학생정보 검색 -->


</mapper>
